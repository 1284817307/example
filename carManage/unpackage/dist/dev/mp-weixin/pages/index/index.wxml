<view class="content"><view class="item"><label class="title">查勘车辆管理系统</label><button class="submit" type="primary" disabled="{{!checked}}" data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" bindtap="__e">还未登录，去登录授权</button><checkbox class="check" value="agree" checked="{{checked}}" data-event-opts="{{[['tap',[['change',['$event']]]]]}}" bindtap="__e">我同意</checkbox><view data-event-opts="{{[['tap',[['show',['$event']]]]]}}" class="text" bindtap="__e">此用户条款</view></view></view>